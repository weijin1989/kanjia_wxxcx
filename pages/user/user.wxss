.header{
  /* height:404rpx; */
  height:254rpx;
  width: 750rpx;
}
.header .top{
  display: flex;
  align-items: center;
  padding:48rpx 26rpx 30rpx 26rpx;
}
.header .top image{
  width: 111rpx;
  height:111rpx;
  border-radius: 50%;
  margin-right:10rpx;
}

.header .top .right{
  flex-grow: 10;
}
.header .top .right text{
  display: block;
  font-size:30rpx;
  font-weight:normal;
  color:rgba(4,0,0,1);
}
.header .top .right label{
  height:43rpx;
  background:rgba(237,237,236,1);
  border-radius:22rpx;
  font-size:24rpx;
  color: #9A9A9A;
  padding:5rpx 15rpx;
}
.header .bottom{
  padding-top:44rpx;
}
.header .bottom .left,.header .bottom .right{
  float:left;
  width: 375rpx;
  text-align: center;
  font-size:28rpx;
  font-weight:500;
  color:rgba(125,124,125,1);
  line-height:56rpx;
}
.header .bottom .right{
  width: 374rpx;
  border-left:1rpx solid #CDCDCD;
}
.order{
  padding:18rpx;
}
.h1{
  font-size:32rpx;
  font-weight:500;
  color:rgba(39,38,38,1);
  line-height:56rpx;
  border-bottom:1px solid #E6E6E5;
  padding-bottom:10rpx;
  position: relative
}
.h1 .span{
  position: absolute;
  right:0;
  top:5rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(166,166,166,1);
}
.h1 .span image{
  width: 11rpx;
  height:18rpx;
  margin-left:10rpx;
}
.nav {
  display: flex;
  text-align: center;
  padding-top:42rpx;
}
.nav .li{
  flex:1;
}
.nav .li image{
  width: 51rpx;
  height:51rpx;
}
.nav .li text{
  display: block;
  font-size:28rpx;
  font-weight:500;
  color:rgba(107,107,107,1);
  line-height:56rpx;
}
.lately_order{
  width:625rpx;
  height:145rpx;
  background:rgba(244,244,244,1);
  border-radius:6rpx;
  margin:0 auto;
  margin-top:29rpx;
  padding:10rpx 25rpx 20rpx 25rpx;
}
.lately_order label{
  font-size:26rpx;
  font-weight:400;
  color:rgba(125,124,125,1);
  line-height:56rpx;
}
.lately_order .ul{
  padding-top:0rpx;
}
.lately_order .ul .li{
  position: relative;
  padding-left:100rpx;
}
.lately_order .ul .li .cp_img{
  position: absolute;
  top:0;
  left:0;
  width: 77rpx;
  height:77rpx;
  border-radius:6rpx;
}
.lately_order .ul .li text{
  font-size:26rpx;
  font-weight:400;
  color:rgba(4,0,0,1);
  line-height:40rpx;
  display: block;
}
.lately_order .ul .li .address{
  position: relative;
  padding-left:24rpx;
  font-size:20rpx;
  font-weight:normal;
  color:rgba(121,122,122,1);
  line-height:24rpx;
}
.lately_order .ul .li .address image{
  position: absolute;
  left:0;
  top:0;
  width: 21rpx;
  height:21rpx;
}
.container{
  padding-bottom:100rpx;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}
 .modalDlg{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.4);
    z-index: 9999;
    /* display: none; */
}
 .modalDlg1{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.8);
    z-index: 9999;
    /* display: none; */
}
.modalContent{
  color:#000;
  width: 400rpx;
  border-radius: 5rpx;
  background-color: #fff;
  line-height:40px;
  padding:0 10rpx;
  position: fixed;
  top: 45%;
  left: 25%;
  z-index: 9999;
}
.getPhoneNumber{
   position: fixed;
  top: 45%;
  left: 25%;
  z-index: 9999;
}
